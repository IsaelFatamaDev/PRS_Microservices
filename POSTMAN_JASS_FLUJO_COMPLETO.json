     "item": [
          {
               "name": "00. Autenticación",
               "item": [
                    {
                         "name": "0.1 Login SUPER_ADMIN",
                         "event": [
                              {
                                   "listen": "test",
                                   "script": {
                                        "exec": [
                                             "if (pm.response.code === 200)  {",
                                             "    var jsonData = pm.response.json();",
                                             "    pm.collectionVariables.set('access_token', jsonData.data.access_token);",
                                             "    pm.collectionVariables.set('refresh_token', jsonData.data.refresh_token);",
                                             "    console.log('Tokens guardados');",
                                             "    console.log('Access Token: ' + jsonData.data.access_token);",
                                             "}"
                                        ],
                                        "type": "text/javascript"
                                   }
                              }
                         ],
                         "request": {
                              "method": "POST",
                              "header": [
                                   {
                                        "key": "Content-Type",
                                        "value": "application/json"
                                   }
                              ],
                              "body": {
                                   "mode": "raw",
                                   "raw": "{\n    \"username\": \"super.admin@jass.gob.pe\",\n    \"password\": \"12345678\",\n    \"clientId\": \"jass-users-service\"\n}"
                              },
                              "url": {
                                   "raw": "{{auth_base_url}}/api/v1/auth/login",
                                   "host": [ "{{auth_base_url}}" ],
                                   "path": [ "api", "v1", "auth", "login" ]
                              },
                              "description": "Login con SUPER_ADMIN. Username: super.admin@jass.gob.pe, Password: documentNumber (12345678)"
                         }
                    },
                    {
                         "name": "0.2 Refresh Token",
                         "event": [
                              {
                                   "listen": "test",
                                   "script": {
                                        "exec": [
                                             "if (pm.response.code === 200) {",
                                             "    var jsonData = pm.response.json();",
                                             "    pm.collectionVariables.set('access_token', jsonData.data.access_token);",
                                             "    pm.collectionVariables.set('refresh_token', jsonData.data.refresh_token);",
                                             "    console.log('Tokens renovados');",
                                             "}"
                                        ],
                                        "type": "text/javascript"
                                   }
                              }
                         ],
                         "request": {
                              "method": "POST",
                              "header": [
                                   {
                                        "key": "Content-Type",
                                        "value": "application/json"
                                   }
                              ],
                              "body": {
                                   "mode": "raw",
                                   "raw": "{\n    \"refreshToken\": \"{{refresh_token}}\",\n    \"clientId\": \"jass-users-service\"\n}"
                              },
                              "url": {
                                   "raw": "{{auth_base_url}}/api/v1/auth/refresh",
                                   "host": [ "{{auth_base_url}}" ],
                                   "path": [ "api", "v1", "auth", "refresh" ]
                              },
                              "description": "Renueva el access token usando el refresh token"
                         }
                    },
                    {
                         "name": "0.3 Logout",
                         "request": {
                              "method": "POST",
                              "header": [
                                   {
                                        "key": "Content-Type",
                                        "value": "application/json"
                                   }
                              ],
                              "body": {
                                   "mode": "raw",
                                   "raw": "{\n    \"refreshToken\": \"{{refresh_token}}\",\n    \"clientId\": \"jass-users-service\"\n}"
                              },
                              "url": {
                                   "raw": "{{auth_base_url}}/api/v1/auth/logout",
                                   "host": [ "{{auth_base_url}}" ],
                                   "path": [ "api", "v1", "auth", "logout" ]
                              },
                              "description": "Cierra la sesión invalidando el token"
                         }
